{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "CMake Build",
      "type": "shell",
      "command": "cmake --build build",
      "group": "build",
      "problemMatcher": ["$gcc"],
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "CMake Configure",
      "type": "shell",
      "command": "cmake -S . -B build",
      "group": "none",
      "problemMatcher": []
    }
  ]
}
